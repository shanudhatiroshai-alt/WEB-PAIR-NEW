{
  "name": "whatsapp-pairing-server",
  "version": "2.0.0",
  "description": "Modern WhatsApp pairing code generator with glassmorphism UI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start server.js --name ShanuFx-md",
    "pm2:stop": "pm2 stop ShanuFx-md",
    "pm2:restart": "pm2 restart ShanuFx-md",
    "pm2:delete": "pm2 delete ShanuFx-md",
    "pm2:logs": "pm2 logs ShanuFx-md"
  },
  "keywords": [
    "whatsapp",
    "pairing",
    "baileys",
    "whatsapp-web"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.0",
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "pino": "^8.16.0",
    "megajs": "^1.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}